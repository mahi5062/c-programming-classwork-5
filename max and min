#include<stdio.h>
int main() 
{
    int arr[] = {30, 48, 66, 27, 80};
    int n = 5;
    int i, max, min;
    max = arr[0];
    min = arr[0];

    for (i = 1; i < n; i++) 
	{
        if (arr[i] > max) 
		{
            max = arr[i];
        }
        if (arr[i] < min) 
		{
            min = arr[i];
        }
    }
    printf("Maximum value in the array is: %d\n", max);
    printf("Minimum value in the array is: %d\n", min);
    return 0;
}
